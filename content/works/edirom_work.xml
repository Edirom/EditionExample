                <notesStmt>
                    <annot type="criticalCommentary">
                        <annot class="#ediromAnnotPrio2 #annotation.category.pausensetzung" type="editorialComment" xml:id="edirom_annotation_00001" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_39 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_40 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_1
                            xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_2">
                            <title xml:lang="de">Generalanmerkung: Pausensetzung</title>
                            <title xml:lang="en"></title>
                            <p>In <rend rend="bold">Ms</rend> sind Viertelpausen häufig liegend notiert. Ganztaktpausen hängen mitunter an der mittleren Notenlinie.</p>
                        </annot>
                        <annot class="#ediromAnnotPrio2 #annotation.category.halsungBalkung" type="editorialComment" xml:id="edirom_annotation_00002" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_43 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_5">
                            <title xml:lang="de">Generalanmerkung: Halsung</title>
                            <title xml:lang="en">General note: necking</title>
                            <p>In <rend rend="bold">Ms</rend> sind die Notenhälse durchgängig rechts am Notenkopf angesetzt.</p>
                        </annot>
                        <annot class="#ediromAnnotPrio3 #annotation.category.rasurTektur" type="editorialComment" xml:id="edirom_annotation_00003" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_39">
                            <title xml:lang="de">Auftakt, Tempobezeichnung</title>
                            <title xml:lang="en"></title>
                            <p><rend rend="bold">Ms</rend> Bassschlüssel über Rasur</p>
                        </annot>
                        <annot class="#ediromAnnotPrio3 #annotation.category.vortragsanweisung" type="editorialComment" xml:id="edirom_annotation_00004" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_39 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_40 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_1
                            xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_2">
                            <title xml:lang="de">Auftakt, Klavier (rechte Hand)</title>
                            <title xml:lang="en"></title>
                            <p><rend rend="bold">Ms</rend> Andante ohne abschließenden Punkt</p>
                        </annot>
                        <annot class="#ediromAnnotPrio3 #annotation.category.dynamik" type="editorialComment" xml:id="edirom_annotation_00005" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_41 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_3">
                            <title xml:lang="de">T. 2 Zz. 4, Klavier</title>
                            <title xml:lang="en"></title>
                            <p>In <rend rend="bold">Ms</rend> steht das pianissimo vor dem Schlag</p>
                        </annot>
                        <annot class="#ediromAnnotPrio2 #annotation.category.artikulation" type="editorialComment" xml:id="edirom_annotation_00006" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_43 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_5">
                            <title xml:lang="de">T. 4 Zz. 3, Klavier (beide Hände)</title>
                            <title xml:lang="en"></title>
                            <p>In <rend rend="bold">Ms</rend> ohne Staccati</p>
                        </annot>
                        <annot class="#ediromAnnotPrio3 #annotation.category.dynamik" type="editorialComment" xml:id="edirom_annotation_00007" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_43 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_5">
                            <title xml:lang="de">T. 4 (Ende)</title>
                            <title xml:lang="en"></title>
                            <p>In <rend rend="bold">Ms</rend> ist am Ende des Taktes in der linken Hand des Klaviers ein Stechereivermerk mit Bleistift nachgetragen</p>
                        </annot>
                        <annot class="#ediromAnnotPrio2 #annotation.category.artikulation" type="editorialComment" xml:id="edirom_annotation_00008" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_44 xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_1.xml#edirom_measure_6">
                            <title xml:lang="de">T. 5 Zz. 1&amp;3, Klavier (rechte Hand)</title>
                            <title xml:lang="en"></title>
                            <p>In <rend rend="bold">Ms</rend> fehlen (vermutlich aus Platzgründen) die Staccati</p>
                        </annot>
                        <annot class="#ediromAnnotPrio3 #annotation.category.rasurTektur" type="editorialComment" xml:id="edirom_annotation_00009" plist="xmldb:exist:///db/apps/edirom/edition-example/content/sources/edirom_source_2.xml#edirom_measure_44">
                            <title xml:lang="de">T. 5 Zz. 3, Klavier (linke Hand)</title>
